{
  "manifest_version": 3,
  "name": "Qpepsi Intermediate",
  "version": "1.0",
  "description": "Upgrade medium to intermediate",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Upgrade medium to intermediate"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}